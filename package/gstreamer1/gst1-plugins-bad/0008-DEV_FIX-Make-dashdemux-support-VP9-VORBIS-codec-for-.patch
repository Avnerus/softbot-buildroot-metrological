From 146165cc2297712d30f34fbfd7283900c48b204c Mon Sep 17 00:00:00 2001
From: linlijuo <linlijuo@realtek.com>
Date: Mon, 13 Feb 2017 20:23:47 +0800
Subject: [PATCH 08/11]  [DEV_FIX] Make dashdemux support VP9/VORBIS codec for
 WEBM container.

Change-Id: Ica1958f9d95e0925c38bfb402c4aac0a1042ce8e
---
 ext/dash/gstmpdparser.c | 4 ++++
 1 file changed, 4 insertions(+)
 mode change 100644 => 100755 ext/dash/gstmpdparser.c

diff --git a/ext/dash/gstmpdparser.c b/ext/dash/gstmpdparser.c
old mode 100644
new mode 100755
index 778ab2e..79cecc0
--- a/ext/dash/gstmpdparser.c
+++ b/ext/dash/gstmpdparser.c
@@ -5610,6 +5610,10 @@ gst_mpdparser_mimetype_to_caps (const gchar * mimeType)
     return "video/quicktime";
   } else if (strcmp (mimeType, "audio/mp4") == 0) {
     return "audio/x-m4a";
+  } else if (strcmp (mimeType, "video/webm") == 0) {
+    return "video/x-vp9";
+  } else if (strcmp (mimeType, "audio/webm") == 0) {
+    return "audio/x-vorbis";	
   } else
     return mimeType;
 }
